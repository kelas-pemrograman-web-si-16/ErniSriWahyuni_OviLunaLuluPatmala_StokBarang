include  ../../users/dashboard
// Content Wrapper. Contains page content
.content-wrapper
    // Content Header (Page header)
    section.content-header
        h1
            | Data Barang
        ol.breadcrumb
            li
                a(href='#')
                    i.fa.fa-dashboard
                    |  Home
            li
                a(href='#') Tables
            li.active Data tables
    // Main content
    section.content
        .row
            .col-xs-12
                // /.box-body
                // /.box
                .box
                    .box-body
                        .box.box-info
                            .box-header.with-border
                                h3.box-title Input data barang
                            // /.box-header
                            // form start
                            form.form-horizontal(method="post", action="/datakeluar", role='form')
                                .box-body
                                    .form-group
                                        label.col-sm-2.control-label(for='kodebarang') Kode Barang
                                        .col-sm-10
                                            input#kodebarang.form-control(type='text' required name="kodebarang" placeholder='Kode Barang')
                                    .form-group
                                        label.col-sm-2.control-label(for='namabarang') Nama Barang
                                        .col-sm-10
                                            input#namabarang.form-control(type='text' name="namabarang" placeholder='Nama Barang')
                                    .form-group
                                        label.col-sm-2.control-label(for='jenisbarang') Jenis Barang
                                        .col-sm-10
                                            input#jenisbarang.form-control(type='text' name="jenisbarang" placeholder='Jenis Barang')



                                // /.box-body
                                .box-footer
                                    button.btn.btn-info.pull-right(type='submit') Input Data
                                // /.box-footer


